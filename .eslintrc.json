{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [/* 
        "airbnb-base" */
       "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "quotes":"off",
        "no-console": "off",
        "no-plusplus": "off",
        "indent": "off"
    }
}
